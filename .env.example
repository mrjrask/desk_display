# Copy this file to `.env` and fill in your real values.
# Never commit the populated `.env` file.

# OpenWeatherMap API keys used by config.py.
# Set the key that matches your Wi-Fi SSID, or fall back to OWM_API_KEY.
OWM_API_KEY=
OWM_API_KEY_VERANO=
OWM_API_KEY_WIFFY=
OWM_API_KEY_DEFAULT=

# Google Maps Distance Matrix API key for the travel screen.
GOOGLE_MAPS_API_KEY=

# Travel screen addresses (optional overrides for config defaults).
TRAVEL_TO_HOME_ORIGIN=
TRAVEL_TO_HOME_DESTINATION=
TRAVEL_TO_WORK_ORIGIN=
TRAVEL_TO_WORK_DESTINATION=
